<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="shakespearQuoter" class="org.mial.training.inter1.impl.ShakespearQuoter">
        <property name="message" value="2 b || ! 2 b"/>
    </bean>

    <bean id="terminatorQuoter" class="org.mial.training.inter1.impl.TerminatorQuoter">
        <property name="messages">
            <list>
                <value>I'll be back</value>
                <value>You are terminated</value>
                <value>Astalavista baby</value>
            </list>
        </property>
    </bean>
</beans>